<body>
    <nav class="navbar navbar-expand-lg">
        <div class="container"> <a class="navbar-brand" href="mailto:damanripu664@gmail.com">Damanripu664@gmail.com</a>
        </div>
    </nav>
    <div class="container">
        <div class="main">
            <table class="table">
                <thead class="thead-dark">
                  <tr>
                    <th scope="col">Serial</th>
                    <th scope="col">Name</th>
                    <th scope="col">Age </th>
                    <th scope="col">Salary</th>
                    <th scope="col">Tax (15% of salary)</th>
                  </tr>
                </thead>
                <tbody>
                    <%var counter=0%>
                    <%Users.forEach(user => {%>
                     <tr>
                        <th scope="row"><%=counter+1%></th>
                        <td><%=user.name%></td>
                        <td><%=user.age%></td>
                        <td><%=user.salary%></td>
                        <td><%=user.tax%></td>
                    </tr>
                  <%counter++%>
                  <% }); %>
                  
                  
                </tbody>
              </table>
              
              
              
        </div>
    </div>
</body>
